{
    "name": "cthulu",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "devDependencies": {
        "@types/jest": "^29.4.0",
        "@typescript-eslint/eslint-plugin": "^5.50.0",
        "@typescript-eslint/typescript-estree": "^5.50.0",
        "eslint": "^8.33.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-prettier": "^4.2.1",
        "husky": "^7.0.0",
        "jest": "^29.4.1",
        "prettier": "^2.8.3",
        "pretty-quick": "^3.1.3",
        "ts-jest": "^29.0.5",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "tsconfig-paths": "^4.1.2",
        "typescript": "^5.9.3"
    },
    "engines": {
        "node": "^16.9.0"
    },
    "scripts": {
        "prod": "pm2 stop ./dist/index.js && ./node_modules/.bin/tsc && pm2 start ./dist/index.js",
        "dev": "ROLE=MOTHER NODE_DEV=true NODE_PATH=src/ ts-node-dev -r tsconfig-paths/register --transpile-only index.ts",
        "commands:deploy": "NODE_DEV=true ts-node-dev -r tsconfig-paths/register --transpile-only scripts/deployCommands.ts",
        "dev:node": "tsc && ROLE=MOTHER NODE_DEV=true NODE_PATH=src/ node ./dist/index.js",
        "test": "tsc && jest",
        "pretty-quick": "pretty-quick --staged --ignore-path=.prettierignore",
        "prettier": "prettier ./ -w",
        "prepare": "husky install"
    },
    "_moduleAliases": {
        "@": "dist/src"
    },
    "dependencies": {
        "@discordjs/builders": "^1.13.0",
        "@discordjs/opus": "^0.9.0",
        "@discordjs/rest": "^1.5.0",
        "@discordjs/voice": "^0.19.0",
        "@distube/ytdl-core": "^4.16.12",
        "@snazzah/davey": "^0.1.7",
        "@types/node": "^18.11.18",
        "axios": "1.6.0",
        "body-parser": "^1.20.0",
        "discord-api-types": "^0.38.31",
        "discord.js": "^14.24.1",
        "dotenv": "^16.0.3",
        "ffmpeg-static": "^5.1.0",
        "libsodium-wrappers": "^0.7.10",
        "lodash": "^4.17.21",
        "module-alias": "^2.2.2",
        "moment": "^2.29.4",
        "mongoose": "7.3.3",
        "multer": "^1.4.5-lts.1",
        "opusscript": "^0.0.7",
        "pm2": "^5.1.2",
        "profanity-util": "^0.2.0",
        "redis": "^4.6.13",
        "socket.io": "^4.7.5",
        "socket.io-client": "^4.7.5",
        "spotify-uri": "^3.0.3",
        "spotify-web-api-node": "^5.0.2",
        "youtube-search": "^1.1.6",
        "ytdlp-nodejs": "^2.3.5"
    }
}
